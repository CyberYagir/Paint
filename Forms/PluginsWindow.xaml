<Window x:Class="Paint.Forms.PluginsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Paint.Forms"
        mc:Ignorable="d"
        Title="Plugins" Height="465.778" Width="285.333" ResizeMode="NoResize" Background="#FF1E1E1E" WindowStartupLocation="CenterScreen">
    <Grid Margin="0">
        <ListBox x:Name="listBox" Margin="0,0,0,48" Background="{x:Null}"/>
        <Button x:Name="OpenFolder" Content="Open Folder" HorizontalAlignment="Left" Margin="9,0,0,10" VerticalAlignment="Bottom" Width="165" Foreground="White" Height="24" Click="OpenFolder_Click" Padding="0" HorizontalContentAlignment="Center" UseLayoutRounding="False" VerticalContentAlignment="Center"/>
        <Button x:Name="Display" Content="Display: All" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="90" Foreground="White" Height="24" Padding="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Click="Display_Click"/>
    </Grid>
</Window>
