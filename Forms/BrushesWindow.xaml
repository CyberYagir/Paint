<Window x:Class="Paint.Forms.BrushesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Paint.Forms"
        mc:Ignorable="d"
        Title="Brush Select" Height="460" Width="310" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Background="#FF1E1E1E">
        <ScrollViewer Height="383" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,393,0,0" TextWrapping="Wrap" Text="Unnamed" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Create" HorizontalAlignment="Left" VerticalAlignment="Top" Width="76" Margin="217,393,0,0" Height="23" Foreground="#FFE6E6E6"/>
        <Button x:Name="button_Copy" Content="File..." HorizontalAlignment="Left" VerticalAlignment="Top" Width="77" Margin="135,393,0,0" Height="23" Foreground="#FFE6E6E6"/>

    </Grid>
</Window>
